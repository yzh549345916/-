<?xml version="1.0" encoding="utf-8"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup>
  <appSettings>
    <!--雪花下落速度-->
    <add key="Speed" value="1"/>
    <!--雪花密度-->
    <add key="SleepTime" value="4000"/>
    <!--雪花最大尺寸-->
    <add key="snowSize" value="40"/>
    <!--雪花容器-->
    <add key="SnowflakeLen" value="4"/>
    <add key="ReSnowflakeLen" value="16"/>

    <!--播放蒙版类型-->
    <add key="mengban" value="3"/>
  </appSettings>
</configuration>
